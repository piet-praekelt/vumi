[run]
branch = True
source = vumi
omit =
    */tests/*
    # vumi.webapp is getting removed (see roadmap).
    vumi/webapp/*

[report]
exclude_lines = 
  pragma: no cover
  def __repr__
  if self\.debug
  raise AssertionError
  raise NotImplementedError
  if 0:
  if __name__ == .__main__.:
ignore_errors = True

[html]
directory = coverage-html
